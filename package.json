{
  "name": "quixplot",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"QuixPlot\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds"
  },
  "author": "CodingBobby",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "function-plot": "^1.19.1",
    "mathjs": "^6.0.2"
  }
}
